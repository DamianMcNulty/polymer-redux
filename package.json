{
  "name": "polymer-redux",
  "version": "0.1.0",
  "description": "Polymer bindings for Redux.",
  "main": "dist/polymer-redux.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "gulp",
    "test": "xo && wct",
    "lint": "polylint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tur-nr/polymer-redux.git"
  },
  "keywords": [
    "redux",
    "polymer",
    "reducer",
    "state",
    "web-components"
  ],
  "author": "Christopher Turner <turner296@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tur-nr/polymer-redux/issues"
  },
  "homepage": "https://github.com/tur-nr/polymer-redux#readme",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-register": "^6.24.0",
    "bower": "^1.5.2",
    "coveralls": "^2.11.9",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "parse5": "^3.0.2",
    "pify": "^2.3.0",
    "polylint": "^2.10.1",
    "polymer-bundler": "^2.0.0-pre.6",
    "polymer-cli": "^0.11.1",
    "redux": "^3.0.5",
    "reselect": "^2.5.1",
    "rollup": "^0.36.0",
    "rollup-plugin-babel": "^2.6.1",
    "run-sequence": "^1.2.2",
    "web-component-tester": "~4.2.2",
    "web-component-tester-istanbul": "https://github.com/t2ym/web-component-tester-istanbul/tarball/0.10.1",
    "xo": "^0.18.0"
  },
  "xo": {
    "next": true,
    "overrides": [
      {
        "files": "gulpfile.babel.js",
        "rules": {
          "indent": [
            "error",
            "tab",
            {
              "MemberExpression": 1
            }
          ]
        }
      },
      {
        "files": "src/*.js",
        "globals": [
          "Polymer"
        ],
        "envs": [
          "browser"
        ]
      }
    ]
  }
}
